{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true,
            "experimentalObjectRestSpread": true,
            "modules": true
        }
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": [
        "eslint:recommended",
        "google",
        "plugin:@typescript-eslint/recommended"
    ],
    "plugins": [
        "react",
        "react-hooks",
        "import",
        "@typescript-eslint"
    ],
    "rules": {
        "semi": [1, "always"],
        "require-jsdoc": 0
    }
}
